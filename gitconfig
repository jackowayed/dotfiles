[github]
	user = jackowayed
[alias]
    st = status -sb
    ci = commit
    br = branch --sort=-committerdate
    co = checkout
    df = diff
    dfc = diff --cached
    dfw = diff --word-diff
    b = branch -a -v
    msg = commit --allow-empty -m
    pl = "log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative"
    gl = "log --graph --pretty=format:'https://github.com/quip/quip/commit/%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative"
    link = "log --graph --pretty=format:'https://github.com/quip/quip/commit/%Cred%h%Creset %s' -n 1"
    pr = "pull --rebase"
    wip = commit -nm
    fast = commit -anm "fast"
    brs = branch
    fixup = commit --amend --no-edit
    melog = log --author daniel

[core]
	editor = emacs -nw -q
	excludesfile = /Users/daniel/.gitignore
[push]
	default = upstream
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = yellow
    changed = green
    untracked = cyan

[user]
	name = Daniel Jackoway
	email = daniel@quip.com
